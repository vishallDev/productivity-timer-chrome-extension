{
    "name": "Productivity Timer",
    "version": "1.0",
    "description": "A simple productivity timer to help you stay focused",
    "manifest_version": 2,
    "permissions": [
      "alarms",
      "notifications"
    ],
    "browser_action": {
      "default_title": "Productivity Timer",
      "default_icon": "icon.png",
      "default_popup": "index.html"
    }
  }
  